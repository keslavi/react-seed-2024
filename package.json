{
  "name": "react-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "startdev": "npm run dev",
    "start": "npm run dev",
    "dev": "vite --port 3000 --host",
    "devWithConsoleDebug": "vite port 3000 --host --inspect --debug",
    "test": "vitest --ui --watch",
    "build": "vite build",
    "build:server": "cd server-koa && npm install && npm run build",
    "build:all": "npm run build && npm run build:server",
    "start:prod": "cd server-koa && npm start",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "installPackages": "npm install lodash moment axios react-router-dom react-redux @reduxjs/toolkit sass yup @hookform/resolvers react-toastify",
    "install:Testing": "npm install -D concurrency vitest jsdom happy-dom @testing-library/jest-dom @testing-library/react @testing-library/user-event",
    "install:Testing2": "echo \"potential firewall issues also requires access to github\" && npm i -D vitest/ui"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/lab": "^6.0.0-beta.14",
    "@mui/material": "^6.1.6",
    "@mui/styled-engine-sc": "^6.1.6",
    "@mui/x-data-grid": "^7.22.2",
    "@reduxjs/toolkit": "^2.2.6",
    "axios": "^1.7.2",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0",
    "react-toastify": "^10.0.5",
    "sass": "^1.77.6",
    "yup": "^1.4.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^3.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "happy-dom": "^17.4.6",
    "jsdom": "^26.1.0",
    "vite": "^5.3.1",
    "vitest": "^3.1.1"
  }
}
