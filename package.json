{
  "name": "react-seed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run copy:mock && vite --port 3000 --host",
    "devWithConsoleDebug": "vite port 3000 --host --inspect --debug",
    "test": "vitest --ui --watch",
    "test:commit": "vitest run",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "build:secure": "node build-secure.js",
    "build:server": "cd server-koa && npm install && npm run build",
    "build:all": "npm run build && npm run build:server",
    "postbuild": "node copy-mock-data.js",
    "copy:mock": "node copy-mock-data.js",
    "start:prod": "cd server-koa && npm run start:prod",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky || true"
  },
  "overrides": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "prop-types": "^15.8.1",
    "@mui/utils": {
      "react-is": "^19.1.0"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.2.0",
    "@mui/styled-engine-sc": "^7.2.0",
    "@mui/utils": "^6.4.9",
    "@mui/x-data-grid": "^8.9.1",
    "@mui/x-date-pickers": "^8.9.2",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "msw": "^2.10.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^7.7.1",
    "react-toastify": "^11.0.5",
    "sass": "^1.89.2",
    "vitest": "^3.2.4",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@swc/plugin-emotion": "^11.0.2",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "vite": "^7.0.4",
    "vite-plugin-static-copy": "^3.1.1"
  }
}
